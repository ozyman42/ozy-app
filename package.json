{
	"name": "root",
  "scripts": {
    "app:deploy:dev": "pnpm --filter @ozy/app app:deploy:dev",
    "app:dev": "pnpm --filter @ozy/app dev",
    "app:docker:dev": "docker built -t ozy_app -f packages/app/Dockerfile . && docker run -t",
    "app:regen:dev": "pnpm --filter @ozy/app app:regen:dev",
    "app:regen:prod": "pnpm --filter @ozy/app app:regen:prod",
    "infra:plan": "pnpm --filter @ozy/infra exec cdktf plan",
    "infra:apply": "pnpm --filter @ozy/infra exec cdktf deploy --auto-approve"
  },
  "dependencies": {
    "socks-proxy-agent": "8.0.2",
    "http-proxy": "1.18.1"
  }
}
